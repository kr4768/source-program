#include <stdio.h>
#include <wiringPi.h>
#include "motor.h"
#include <softPwm.h>

int main(void){
 if(wiringPiSetup() == -1){return 1;}
 softPwmCreate(FAN_IA, 0, 1000);
 while(1){
  int press;
  scanf("%c", &press);

  if(press=='q'){
   Stop();
  }
  if(press=='w'){
   Weak();
  }
  if(press=='m'){
   Medium();
  }
  if(press=='s'){
   Strong();
  }
 }
 return 0;
}

